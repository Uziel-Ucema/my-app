	"Contact" [shape=Mrecord, label="{Contact|subject :varchar\lemail :varchar\lmessage :TEXT\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lid :INTEGER\l}"]
	"Folder" [shape=Mrecord, label="{Folder|name :varchar\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lid :INTEGER\l}"]
	"ImageMetadata" [shape=Mrecord, label="{ImageMetadata|breed :varchar\limage_name :varchar\llocation :varchar\ltime :varchar\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lid :INTEGER\l}"]
	"Location" [shape=Mrecord, label="{Location|name :varchar\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lid :INTEGER\l}"]
	"PetSubmission" [shape=Mrecord, label="{PetSubmission|title :varchar\lbody :TEXT\limage :varchar\luser_id :INTEGER\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lclosed :boolean\lid :INTEGER\l}"]
