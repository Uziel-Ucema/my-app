	"Folder" -> "Subscription" [arrowtail=crow, arrowhead=crow, dir=both color="#12B917"]
	"Folder" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#5482CC"]
	"Location" -> "Subscription" [arrowtail=crow, arrowhead=crow, dir=both color="#71A678"]
	"Location" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#0CA678"]
	"PetSubmission" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#9A7334"]
	"PetSubmission" -> "ActiveStorage::Attachment" [label="image_attachment", arrowtail=crow, arrowhead=crow, dir=both color="#449519"]
	"PetSubmission" -> "ActiveStorage::Blob" [label="image_blob", arrowtail=crow, arrowhead=crow, dir=both color="#89A950"]
	"Subscription" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#754192"]
